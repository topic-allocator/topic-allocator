{
  "name": "@lti/server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --outdir=dist --platform=node --external:@azure/functions-core --bundle --minify --sourcemap",
    "build-seed": "esbuild src/seed/ci-seed.ts --outdir=dist/seed --platform=node --external:@azure/functions-core --bundle --minify --sourcemap",
    "prestart": "npm run build",
    "start": "func start",
    "start:ci": "prisma migrate deploy && node dist/seed/ci-seed.js && func start",
    "dev": "nodemon --exec \"npm run start\" --watch src --ext ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --check src --log-level warn",
    "publish": "func azure functionapp publish lti-linux"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0-alpha.1",
    "@prisma/client": "^5.3.1",
    "@trpc/server": "^11.0.0-rc.467",
    "azure-functions-core-tools": "^4.x",
    "jsonwebtoken": "^9.0.0",
    "superjson": "^2.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "esbuild": "^0.23.0",
    "eslint": "^8.41.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "read-excel-file": "^5.8.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
